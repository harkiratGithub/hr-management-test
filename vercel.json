{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/employee-recruitment-dashboard/browser",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://employee-backend-pcq1.onrender.com/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}


